pipeline {
  agent any
  stages {
    stage('deploy') {
      steps {
        echo 'Start Pull And Push Images'
        sh 'echo ababwer >> /new.txt; cp /new.txt /sec.txt'
        sh 'git clone https://github.com/reyhsu/docker-gitlab.git'
        archiveArtifacts 'XshellPortable.rar'
      }
    }
  }
}